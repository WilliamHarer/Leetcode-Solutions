class Solution(object):
    def findNumbers(self, nums):
        if nums is None:
            return 0
        count=0
        for  i in range(0,len(nums)):
            if len(str(nums[i])) %2 == 0:
                count=count+1
        return count
        """
        :type nums: List[int]
        :rtype: int
        """
        
