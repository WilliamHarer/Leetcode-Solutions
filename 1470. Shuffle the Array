class Solution(object):
    def shuffle(self, nums, n):
        if n==0:
            return nums
        returnList=[]
        i=0
        j=n
        while j<len(nums):
            returnList.append(nums[i])
            returnList.append(nums[j])
            i+=1
            j+=1
        return returnList
    
        """
        :type nums: List[int]
        :type n: int
        :rtype: List[int]
        """
        
